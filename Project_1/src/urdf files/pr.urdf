<?xml version="1.0"?>
<robot name="origins">
  <link name="base_link">
    <visual>
      <geometry>
        <box size="1 1 1"/>
      </geometry>
    </visual>
  </link>

  <link name="link2">
    <visual>
      <geometry>
        <box size="0.1 0.1 2"/>
      </geometry>
    </visual>
  </link>

  <link name="link3">
    <visual>
      <origin rpy="0 0 0" xyz="0 0.5 0"/>
      <geometry>
        <box size="0.1 1 0.1"/>
      </geometry>
    </visual>
  </link>

<joint name="r" type="revolute">
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" lower="-3.14" upper="3.14" velocity="0.5"/>
    <origin rpy="0 0 0" xyz="0 0 1"/>
    <parent link="link2"/>
    <child link="link3"/>
  </joint>

<joint name="p" type="prismatic">
    <parent link="base_link"/>
    <child link="link2"/>
    <limit effort="1000.0" lower="-1" upper="0" velocity="0.5"/>
    <origin rpy="0 0 1" xyz="0 0 1.5"/>
    <axis xyz="0 0 1"/>
  </joint>

</robot>



